[alias]
    a = add
    c = commit -m
    ca = commit --amend
    co = checkout
    cb = checkout -b
    hide = update-index --assume-unchanged
    unhide = update-index --no-assume-unchanged
    lol = log --graph --oneline
    pl = pull
    ps = push
    psf = push --force-with-lease
    psu = push --set-upstream origin
    s = status -s -b
    st = stash
